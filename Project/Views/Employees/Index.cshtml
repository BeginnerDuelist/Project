@model List<Project.Models.Domain.Employee>
@{
}
<h1>Employees</h1>
<table class="table">
     <thead>
          <tr>
               <th>Id</th>
               <th>Name</th>
               <th>Email</th>
               <th>BirthDate</th>
               <th>Department</th>
          </tr>
     </thead>
     <tbody>
          @foreach (var employee in Model)
          {
               <tr>
                    <td>@employee.Id</td>
                    <td>@employee.Name</td>
                    <td>@employee.Email</td>
                    <td>@employee.BirthDate</td>
                    <td>@employee.Department</td>
                    <td><a class="btn btn-primary" href="Employees/View/@employee.Id">View</a></td>
               </tr>
          }

     </tbody>
</table>
